{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/scheduled-task",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "crons": [
    {
      "path": "/scheduled-task",
      "schedule": "0 * * * *"
    }
  ]
}
