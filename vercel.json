{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "crons":[
    {
      "path": "https://task-manager-application-puce.vercel.app/scheduled-task",
      "schedule": "0 * * * *"
    }
  ]
}
